  "devDependencies": {
    "basic-auth": "^1.0.3",
    "brfs": "^1.4.0",
    "cookie-parser": "^1.3.5",
    "express": "^4.13.0",
    "node-fetch": "^1.3.2",
    "tape": "^4.0.0",
    "ua-parser-js": "^0.7.7"
  }
}
