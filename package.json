{
  "name": "stream-http",
  "version": "1.3.0",
  "description": "Streaming http in the browser",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/jhiesey/stream-http.git"
  },
  "scripts": {
    "test": "npm run test-node && npm run test-browser",
